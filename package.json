{
  "name": "@andrew-codes/interview-sandbox",
  "version": "1.0.0",
  "private": true,
  "packageManager": "yarn@4.9.2",
  "engines": {
    "node": ">=22.14.0"
  },
  "scripts": {
    "start": "vite",
    "test/component": "concurrently 'cypress open --component --browser edge' 'yarn react-devtools'",
    "test/component/ci": "cypress run --component --browser edge",
    "test/component/update": "(yarn run test/component || true) && cypress-image-diff-html-report start --reportJsonDir visual-tests/report --autoOpen",
    "test/unit": "jest --config jest.config.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@jest/globals": "^30.0.5",
    "@types/cypress": "^1.1.6",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.2.0",
    "cypress": "^14.5.3",
    "cypress-cdp": "^1.6.82",
    "cypress-image-diff-html-report": "^2.2.0",
    "cypress-image-diff-js": "^2.4.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^30.0.5",
    "jsonc-eslint-parser": "^2.4.0",
    "prettier": "^3.6.2",
    "react-devtools": "^6.1.5",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.38.0",
    "vite": "^6.3.5"
  }
}
